{
  "name": "restify-orm-scaffold",
  "version": "0.0.30",
  "description": "Sample REST API scaffold built on Node.JS in TypeScript with restify and TypeORM (with Waterline and Sequelize support also).",
  "repository": {
    "url": "https://github.com/SamuelMarks/restify_waterline_scaffold"
  },
  "license": "(Apache-2.0 OR MIT)",
  "author": "Samuel Marks @SamuelMarks",
  "main": "main.js",
  "scripts": {
    "build": "rm -rf node_modules; typings i; npm i --unsafe-perms; tsc",
    "build_start": "rm -rf node_modules; [ -f typings.json ] && typings i; npm i && tsc && node main.js | bunyan",
    "start": "node main.js | bunyan",
    "test": "mocha test --recursive"
  },
  "dependencies": {
    "@offscale/custom-restify-errors": "^0.0.4",
    "@offscale/nodejs-utils": "^0.0.6",
    "@offscale/orm-mw": "^0.0.3",
    "@offscale/restify-validators": "^0.0.3",
    "@offscale/routes-merger": "^0.0.3",
    "argon2": "0.24.0",
    "async": "^3.1.0",
    "bunyan": "^1.8.12",
    "faker": "^4.1.0",
    "hiredis": "^0.5.0",
    "ioredis": "^4.10.0",
    "pg": "^7.11.0",
    "redis": "^2.8.0",
    "restify": "^8.3.3",
    "restify-errors": "^8.0.0",
    "tv4": "^1.3.0",
    "typeorm": "^0.3.0-alpha.23",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/bunyan": "1.8.6",
    "@types/chai": "^4.1.7",
    "@types/faker": "^4.1.5",
    "@types/ioredis": "4.0.12",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "@types/redis": "^2.8.13",
    "@types/restify": "^7.2.11",
    "@types/restify-errors": "^4.3.3",
    "@types/superagent": "^4.1.1",
    "@types/supertest": "^2.0.7",
    "@types/tv4": "^1.2.29",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "chai-json-schema": "^1.5.1",
    "chai-json-schema-ajv": "^5.0.0",
    "mocha": "^6.1.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "typescript": "~3.5.2"
  }
}
