    "@types/restify-errors": "^4.3.3",
    "@types/superagent": "^4.1.1",
    "@types/supertest": "^2.0.7",
    "@types/tv4": "^1.2.29",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
